          <artifactId>spring-boot-starter-actuator</artifactId>
          <version>1.5.7.RELEASE</version>
          <scope>provided</scope>
        </dependency>
      </dependencies>
      <properties>
        <spring.profiles.active>development</spring.profiles.active>
      </properties>
    </profile>
